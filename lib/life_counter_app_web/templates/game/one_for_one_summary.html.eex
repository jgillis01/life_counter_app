<div class="row">
  <h2 class="col text-center">Game Summary</h2>
</div>
<div class="row" id="player-summary">
  <table class="table table-striped">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Points</th>
      <tr>
    </thead>
    <tbody>
      <tr v-for="player in players">
        <td>{{ player.name }}</td>
        <td>{{ player.points }}</td>
      </tr>
    </tbody>
  </table>
</div>

